{% extends "layout.twig" %}

{% block content %}


<header class="bk-header">
	<div class="companyheader clearfix">
		{{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
		{{widget('companyname', 'companyname', {"business": "profile"})|raw}}
	</div>
	{{widget('extendednavigation', 'navigationheader', {"align":"right"})|raw}}
</header>


<section class="bk-section">
	{{widget('feature', 'featurehome', {"header": "profile", "strapline":"profile", "buttonText":"Nullam Posuer","defaultBgImg": asset('images/feature-bg.png'), 'positionTopPercentage': '50','showBtn': true})|raw}}
</section>


<div class="main container clearfix">
	{{zone('main')|raw}}
</div>


<footer class="bk-footer">
	<div class="container clearfix">
		<div class="footerleft">
		{{widget('profile', 'footerprofileleft', { 'profileType' : 'socialicons' })|raw}}
		</div>
		<div class="footerright">
		{{widget('profile', 'footerprofileright', { 'profileType':'copyright', 'copyright':'profile'})|raw}}
		</div>
	</div>
</footer>


{% endblock %}
